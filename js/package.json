{
    "name": "@ravoni4devs/libcryptus",
    "version": "1.0.0",
    "description": "A lightweight and efficient library providing essential cryptographic utilities",
    "main": "dist/cryptus.js",
    "types": "dist/cryptus.d.ts",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "npx rollup --bundleConfigAsCjs --makeAbsoluteExternalsRelative -c && npm run definitions",
        "bundle": "cat argon2-bundled.min.js dist/libcryptus-cjs.js > dist/bundle.min.js",
        "definitions": "npx tsc",
        "test": "jest",
        "deploy": "npm publish --access public"
    },
    "author": "Ravoni Company",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/preset-env": "^7.28.5",
        "@babel/register": "^7.28.3",
        "@peculiar/webcrypto": "^1.5.0",
        "@rollup/plugin-babel": "^6.1.0",
        "@rollup/plugin-commonjs": "^28.0.9",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "babel-jest": "^30.2.0",
        "chai": "^6.2.0",
        "jest": "^30.2.0",
        "jsdom": "^27.0.1",
        "rollup": "^4.52.5",
        "typescript": "^5.9.3",
        "util": "^0.12.5"
    },
    "dependencies": {
        "argon2-browser": "^1.18.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "cryptography",
        "encryption",
        "decryption",
        "hashing"
    ],
    "files": [
        "dist"
    ]
}
