{
    "name": "@ravoni4devs/libcryptus",
    "version": "0.0.9",
    "description": "A lightweight and efficient library providing essential cryptographic utilities",
    "main": "dist/cryptus.js",
    "types": "dist/cryptus.d.ts",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "npx rollup --bundleConfigAsCjs --makeAbsoluteExternalsRelative -c && npm run definitions",
        "bundle": "cat argon2-bundled.min.js dist/libcryptus-cjs.js > dist/bundle.min.js",
        "definitions": "npx tsc",
        "test": "jest",
        "deploy": "npm publish --access public"
    },
    "author": "Ravoni Company",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.24.8",
        "@babel/preset-env": "^7.23.9",
        "@babel/register": "^7.23.7",
        "@peculiar/webcrypto": "^1.4.5",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "babel-jest": "^29.7.0",
        "chai": "^5.0.3",
        "crypto": "^1.0.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^24.0.0",
        "rollup": "^4.18.1",
        "typescript": "^5.8.3",
        "util": "^0.12.5"
    },
    "dependencies": {
        "argon2-browser": "^1.18.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "cryptography",
        "encryption",
        "decryption",
        "hashing"
    ],
    "files": [
        "dist"
    ]
}
